<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="SampleMapper">

<resultMap id="sample" type="com.Joker.api.sample">
		<result property="data" column="data" />
		</resultMap>
		
		<select id="getData" resultType="String" parameterType="String">
		SELECT name FROM helloWorld WHERE name = #{data};
		</select>
		</mapper>

		
		